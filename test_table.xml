<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
    <meta>
        <sampleQuery>select * from html where url="http://finance.yahoo.com/q?s=yhoo" and
            xpath='//div[@id="yfi_headlines"]/div[2]/ul/li/a'
        </sampleQuery>
    </meta>
    <bindings>
        <select>
            <inputs>
                <key name="url" required="true" type="xs:string"/>
                <key name="charset" type="xs:string"/>
                <key name="browser" type="xs:boolean"/>
                <key default="/html/body" name="xpath" type="xs:string"/>
                <key
                        description="valid values for compat is 'html5' and 'html4'"
                        name="compat" type="xs:string"/>
            </inputs>
        </select>        
    </bindings>
</table>

